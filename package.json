{
  "name": "drust",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "predev": "pnpm run sanity-typegen",
    "dev": "remix vite:dev",
    "prebuild": "pnpm run sanity-typegen",
    "build": "remix vite:build",
    "format": "npx prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "eslint --cache --cache-location ./node_modules/.cache/eslint --fix .",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "sanity-typegen": "npx sanity schema extract --path ./types/sanity.schema.json && npx sanity typegen generate",
    "sanity-typegen:watch": "chokidar \"**/*.schema.ts\" \"**/*.queries.ts\" --ignore \"types/sanity.types.ts\" -c \"pnpm run sanity-typegen\""
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@portabletext/react": "^3.1.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@remix-run/node": "^2.12.1",
    "@remix-run/react": "^2.12.1",
    "@remix-run/serve": "^2.12.1",
    "@resvg/resvg-js": "^2.6.2",
    "@sanity/asset-utils": "^1.3.0",
    "@sanity/block-tools": "^3.56.0",
    "@sanity/client": "^6.21.3",
    "@sanity/icons": "^3.4.0",
    "@sanity/image-url": "^1.0.2",
    "@sanity/preview-url-secret": "^1.6.20",
    "@sanity/react-loader": "^1.10.6",
    "@sanity/schema": "^3.56.0",
    "@sanity/ui": "^2.8.9",
    "@sanity/vision": "^3.56.0",
    "@sanity/visual-editing": "^2.1.9",
    "@vercel/remix": "^2.11.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "groq": "^3.56.0",
    "isbot": "^5.1.17",
    "jsdom": "^25.0.0",
    "lucide-react": "^0.437.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rxjs": "^7.8.1",
    "sanity": "^3.56.0",
    "satori": "^0.10.14",
    "styled-components": "^6.1.13",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.9.0",
    "@remix-run/dev": "^2.12.1",
    "@remix-run/eslint-config": "^2.12.1",
    "@storybook/addon-essentials": "^8.3.4",
    "@storybook/addon-interactions": "^8.3.4",
    "@storybook/addon-links": "^8.3.4",
    "@storybook/addon-onboarding": "^8.3.4",
    "@storybook/blocks": "^8.3.4",
    "@storybook/react": "^8.3.4",
    "@storybook/react-vite": "^8.3.4",
    "@storybook/test": "^8.3.4",
    "@tailwindcss/typography": "^0.5.15",
    "@thoughtbot/eslint-config": "^1.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/styled-components": "^5.1.34",
    "autoprefixer": "^10.4.20",
    "chokidar-cli": "^3.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^0.9.0",
    "husky": "^9.1.6",
    "postcss": "^8.4.42",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "storybook": "^8.3.4",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
